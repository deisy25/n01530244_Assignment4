@model IEnumerable<Assignment3.Models.Student>
@{
    ViewBag.Title = "List";
}

<div id="containerlist">
    <a href="/Home/Index">Back to Main Menu</a>

    <h3>List of Students</h3>
    <p>There are @Model.Count() students in the system</p>

    @foreach (var student in Model)
    {
        <div>
            <a href="/Student/Show/@student.StudentID">@student.StudentName</a>
        </div>
    }
</div>